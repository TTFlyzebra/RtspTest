<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="-146px"
    android:layout_marginTop="-106px"
    android:background="#000"
    tools:context="com.longhorn.dvrexplorer.DVRActivity">

    <tcking.github.com.giraffeplayer.IjkVideoView
        android:id="@+id/file_video_vv"
        android:layout_width="788px"
        android:layout_height="442px"
        android:layout_marginLeft="318px"
        android:layout_marginTop="133px" />

    <SeekBar
        android:id="@+id/file_video_sk"
        android:layout_width="578px"
        android:layout_height="12px"
        android:layout_marginLeft="423px"
        android:layout_marginTop="534px"

        android:thumb="@android:color/transparent" />

    <TextView
        android:id="@+id/file_video_sktv1"
        android:layout_width="80px"
        android:layout_height="24px"
        android:layout_marginLeft="338px"
        android:layout_marginTop="524px"
        android:gravity="right"
        android:textColor="#FFF"
        android:textSize="22px" />

    <TextView
        android:id="@+id/file_video_sktv2"
        android:layout_width="80px"
        android:layout_height="24px"
        android:layout_marginLeft="1006px"
        android:layout_marginTop="524px"
        android:gravity="left"
        android:textColor="#FFF"
        android:textSize="22px" />

    <ImageView
        android:id="@+id/file_video_iv"
        android:layout_width="788px"
        android:layout_height="442px"
        android:layout_marginLeft="318px"
        android:layout_marginTop="133px" />

    <Button
        android:id="@+id/file_pho_return"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="290px"
        android:layout_marginTop="598px"
        android:background="@drawable/file_pho_return"
        android:enabled="true" />

    <Button
        android:id="@+id/file_video_left"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="525px"
        android:layout_marginTop="602px"
        android:background="@drawable/file_video_left"
        android:enabled="true" />

    <Button
        android:id="@+id/file_video_play"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="662px"
        android:layout_marginTop="602px"
        android:background="@drawable/file_video_play"
        android:enabled="true" />

    <Button
        android:id="@+id/file_video_right"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="798px"
        android:layout_marginTop="602px"
        android:background="@drawable/file_video_right"
        android:enabled="true" />
</RelativeLayout>
